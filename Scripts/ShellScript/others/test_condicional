# realizando teste de status
test "Lenon" = "Lenon"

# verificando variavel
echo $?

# verifica se variavel é nula
test -z "$var"

test "1" -eq "1"	# saida 0
test "1" -ne "2"	# saida 0

-eq     É Igual
-ne     Não é igual
-ge     É maior ou Igual
-gt     É Maior que
-lt      É Meno que
-le     É menor ou Igual